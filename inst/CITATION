citHeader("To cite GDAtools in publications, use:")

bibentry(
  bibtype  = "manual",
  title    = "{descriptio}: Descriptive Analysis of Associations between two Variables in {R}",
  author   = "Nicolas Robette",
  year     = "2023",
  note     = "R package version 1.3",
  url      = "https://nicolas-robette.github.io/descriptio/",
  
  textVersion = paste("Robette N. (2023).",
        "descriptio: Descriptive Analysis of Associations between two Variables in R.",
        "version 1.3.",
        "https://nicolas-robette.github.io/descriptio/")
)

citFooter("See",sQuote("citation()"),"for citing R itself.")

